{
  "name": "workout-backend",
  "version": "1.0.0",
  "description": "Create and share Workout routines",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "ts-node src/server.ts",
    "start:dev": "nodemon 'src/server.ts' --exec 'ts-node' src/server.ts -e ts,graphql --ignore 'src/types.d.ts' ",
    "fmt": "prettier --write",
    "build": "tsc",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^3.1.1",
    "apollo-server-express": "^3.3.0",
    "axios": "^0.21.4",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "graphql": "^15.6.0",
    "graphql-upload": "^12.0.0",
    "helmet": "^4.6.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@peergrade/graphql-schema-typescript": "^1.5.2-next.1",
    "@types/graphql": "14.5.0",
    "@types/node": "16.10.1",
    "@types/sharp": "^0.29.2",
    "@types/ws": "8.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "prisma": "^3.1.1",
    "ts-node": "10.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "4.4.3"
  },
  "keywords": [],
  "author": "Henrique Ramos <henriquealberone@outlook.com>",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,css,md}": [
      "prettier --write", "tslint --fix",
      "git add"
    ]
  }
}
